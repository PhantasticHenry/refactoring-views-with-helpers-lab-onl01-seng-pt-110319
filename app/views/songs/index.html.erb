<ul>
<% @songs.each do |song| %>
  <% if !song.artist.present? %>
  <li><%= link_to song.title, song_path(song) %> - <%= link_to "Add Artist", edit_song_path(song) %></li><br>
  <% else %>
  <%= link_to song.title, song_path(song) %><br>
  <% end %>
<%= display_artist(song) %><br>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
